<!DOCTYPE html>
<html>
<%= render 'chartHead' %>
<body>
	<%= render 'header' %>


<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>
  

  <tbody>
    <% @job.each do |job_site| %>
      
	      <tr>
	      	<td><%= job_site.job_name %></td>
	      </tr>
	      <tr>
	      	<%= @booty = @rain.where(:job_sites_id => job_site.id).last(4).collect { |obj| obj.amount } %>
	      	<%= content_tag 'td', class: 'rains', data: {:rain=> @booty, :site_id=> 34} do %>
	      		rsatras
	      	<% end %>
	      </tr>
	      <tr>
			<td><%= job_site.job_number %></td>
		  </tr>
		  <tr>
		  	<td><%= job_site.abbr %></td>
		  </tr>
		  <tr>
		  	<td><%= job_site.zip_code %></td>
		  </tr>
		  <tr>
		 	<td><%= job_site.cross_street %></td>
		  </tr>
		  <tr>
	        <td><%= link_to('Show', job_site_path(job_site)) %></td>
	        <td><%= link_to('Edit', edit_job_site_path(job_site)) %></td>
	        <td><%= link_to('Delete', :action => 'destroy', :id => job_site.id) %></td>
	      </tr>
	<%end%>
  </tbody>
</table>

<br>
The Rainrate for <%=@coordinates%> (Henderson, NV) is <%=@testresults['rainRate']%>
<%= link_to 'New Job site', new_job_site_path %>
</br>
</br>

<!-- ['rainRate'] -->
</br>
</br>
<%= @jobobject %>
</br>
</br>
<%= @rainobject %>

<div class="page-wrapper">
  
	<section class="container">

		  <div class="row">
		    <%= render 'calendar' %>
		    <%= react_component 'Main', { data: @job } %>
		  </div><!-- row -->
      
	</section>
</div>




</body>
</html>	